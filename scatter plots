import numpy as np
import matplotlib.pyplot as plt

#involves plotting individual data points
x = np.random.rand(50)
y = np.random.rand(50)

#x = [10,20,30,40,50]
#y = [5,7,3,2,6]


#change color,marker(x,triangle,circle),
plt.scatter(x,y,c = ' red',marker = 'x')
plt.show()

#Mostly important for linear regression
